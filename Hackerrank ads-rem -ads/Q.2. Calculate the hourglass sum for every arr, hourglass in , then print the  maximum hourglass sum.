#include <stdio.h>
#include <limits.h> // for INT_MIN

int main() {
    int arr[6][6];
    
    // Input 6x6 array
    for (int i = 0; i < 6; i++) {
        for (int j = 0; j < 6; j++) {
            scanf("%d", &arr[i][j]);
        }
    }
    
    int maxSum = INT_MIN; // start with smallest possible integer

    // Loop through all possible hourglasses
    for (int i = 0; i <= 3; i++) {
        for (int j = 0; j <= 3; j++) {
            int sum = arr[i][j] + arr[i][j+1] + arr[i][j+2]
                                + arr[i+1][j+1] +
                      arr[i+2][j] + arr[i+2][j+1] + arr[i+2][j+2];
            
            if (sum > maxSum)
                maxSum = sum;
        }
    }

    printf("%d", maxSum);
    return 0;
}
