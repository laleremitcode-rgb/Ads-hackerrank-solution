
#include <stdio.h>
int main() {
    int n;
    scanf("%d", &n);  // number of elements

    int arr[n];
    for (int i = 0; i < n; i++) {
        scanf("%d", &arr[i]);  // input array elements
    }

    // print reversed array
    for (int i = n - 1; i >= 0; i--) {
        printf("%d", arr[i]);
        if (i > 0) printf(" "); // space between elements
    }

    return 0;
}

