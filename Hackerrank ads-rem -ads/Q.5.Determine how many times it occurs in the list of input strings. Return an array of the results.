#include <stdio.h>
#include <string.h>

int main() {
    int n;
    scanf("%d", &n);

    char strings[n][100];  // assuming max string length = 99
    for (int i = 0; i < n; i++) {
        scanf("%s", strings[i]);
    }

    int q;
    scanf("%d", &q);

    char query[100];
    for (int i = 0; i < q; i++) {
        scanf("%s", query);
        int count = 0;

        // compare each input string with current query
        for (int j = 0; j < n; j++) {
            if (strcmp(strings[j], query) == 0) {
                count++;
            }
        }

        printf("%d\n", count);
    }

    return 0;
}
